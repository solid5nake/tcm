{
  "name": "react-transform-boilerplate",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "test:watch": "./node_modules/karma/bin/karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/gaearon/react-transform-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-transform-boilerplate",
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-eslint": "5.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "cross-env": "1.0.8",
    "enzyme": "^2.4.1",
    "eslint": "1.10.3",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "3.16.1",
    "eventsource-polyfill": "0.9.6",
    "express": "^4.13.3",
    "jasmine-core": "^2.5.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "rimraf": "^2.4.3",
    "watchify": "^3.7.0",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1"
  },
  "dependencies": {
    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2"
  }
}
